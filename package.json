{
  "name": "@hrimthurs/tackle",
  "version": "1.16.0",
  "description": "Various auxiliary tools for working in JavaScript projects",
  "author": "@hrimthurs",
  "license": "MIT",
  "repository": "hrimthurs/Tackle",
  "homepage": "https://github.com/hrimthurs/Tackle#readme",
  "keywords": [
    "javascript",
    "tools",
    "routines"
  ],
